@Scripts.Render("~/bundles/ngReport")
<div class="tab-pane active" ng-controller="AssociationFinancialReportController">
    <div class="row" >       
        <form name="frm" novalidate>
            <div class="col-md-12">
                <h3 class="form-section">Association Financial Report</h3>
            </div>
            <div class="col-md-12">
                <div class="portlet box">
                    <div class="portlet-body form">
                        <div class="form-horizontal">
                            <div class="form-body">
                                <!--Content-->
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Start Date</label>
                                    <div class="col-md-6">
                                        <div class="input-group input-medium date date-picker">
                                            <input type="text" class="form-control" readonly="" name="StartDate" datetime-picker="mm/dd/yyyy" ng-model="PaymentHistory.StartDate">
                                            <span class="input-group-btn">
                                                <button class="btn default" type="button">
                                                    <i class="fa fa-calendar"></i>
                                                </button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">End Date</label>
                                    <div class="col-md-6">
                                        <div class="input-group input-medium date date-picker">
                                            <input type="text" class="form-control" readonly="" name="StartDate" datetime-picker="mm/dd/yyyy" ng-model="PaymentHistory.EndDate">
                                            <span class="input-group-btn">
                                                <button class="btn default" type="button">
                                                    <i class="fa fa-calendar"></i>
                                                </button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
        <div class="col-md-7">
            <div class="portlet box">
                <div class="portlet-body ">
                    <div class="form-horizontal">
                        <div class="clearfix">
                            <div class="col-md-offset-8 col-md-7">
                                <button type="button" class="btn blue btn-lg" ng-click="GenerateReport(frm.$invalid)">Generate Report </button>
                                <button type="button" class="btn blue btn-lg">Cancel </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
         <table class="table table-striped table-bordered table-hover" id="tblUser" aa-data="AssociationFinancialReportList" paging-table>             
        </table>
    </div>
</div>
