 @Scripts.Render("~/bundles/ngReport")
<div class="tab-pane active" ng-controller="OutstandingBalanceController">
    <div class="row" >
        <form name="frm" novalidate>
            <div class="col-md-12">
                <h3 class="form-section">Outstanding Balances</h3>
            </div>
        </form>
        <div class="col-md-7">
            <div class="portlet box">
                <div class="portlet-body ">
                    <div class="form-horizontal">
                        <div class="clearfix">
                            <div class="col-md-offset-8 col-md-7">
                                <button type="button" class="btn blue btn-lg" ng-click="GenerateReport()">Generate Report </button>
                                <button type="button" class="btn blue btn-lg">Cancel </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <table class="table table-striped table-bordered table-hover" id="tblUser" aa-data="DataSourceList" paging-table>             
        </table>
    </div>
</div>
