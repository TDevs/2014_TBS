@{   
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/bundles/nguniversalagent")
<div ng-controller="RTAUploadController">
    <div class="row">
        <div class="col-md-12">
            <div class="portlet box grey">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-reorder"></i>
                        Agent Cashiering Uploading Sheet
                    </div>
                </div>
                <div class="portlet-body form">
                    <form class="form-horizontal form-bordered" action="#">
                        <div class="form-body">
                            <div class="form-group">
                                <label class="control-label col-md-3">File</label>
                                <div class="col-md-9">
                                    <div class="fileinput fileinput-new" style="display:block;">      
                                        <span class="input-group-addon btn default btn-file">
                                            <span class="fileinput-new">Select file </span>
                                            <input type="file" name="..." id="divUploadImage" style="width:200px;margin-right:50px;">
                                        </span>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="portlet box grey">
                <!-- Table Result Of Search-->
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-bars"></i>Result Agent Cashiering
                    </div>
                </div>
                <div class="portlet-body">
                    <table class="table table-striped table-bordered table-hover" id="tblBank">
                        <thead>
                            <tr>
                                <th>Date
                                </th>
                                <th>Client Name
                                </th>
                                <th>ID #
                                </th>
                                <th>Pick up Address         
                                </th>
                                <th>Due Time
                                </th>
                                <th>P/U Time
                                </th>
                                <th>D/O Time
                                </th>
                                <th>Drop Off Address</th>
                                <th>Phone       
                                </th>
                                <th>CAB
                                </th>
                                <th>CL
                                </th>
                                <th>AMT
                                </th>
                                <th>Paid Date</th>
                                <th>Confirm</th>
                            </tr>

                        </thead>
                        <tbody>
                            <tr ng-show="RTAList.length<=0">
                                <td colspan="14" style="text-align: center">No Records </td>
                            </tr>
                            <tr ng-repeat="rta in RTAList">
                                <td>{{rta.DateRun}}
                                </td>
                                <td>{{rta.ClientName}}
                                </td>
                                <td>{{rta.LicenseNumber}}
                                </td>
                                <td>{{rta.PickupAddress}}
                                </td>
                                <td>{{rta.DueTime}}
                                </td>
                                <td>{{rta.PUTime}}
                                </td>
                                <td>{{rta.DOTime}}
                                </td>
                                <td>{{rta.DropOffAddress}}
                                </td>
                                <td>{{rta.Phone}}</td>
                                <td>{{rta.CAB}}
                                </td>
                                <td>{{rta.CL}}
                                </td>
                                <td>{{rta.Amount}}
                                </td>
                                <td>{{rta.PaidDate}}
                                </td>
                                <td>{{rta.Confirmed}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
