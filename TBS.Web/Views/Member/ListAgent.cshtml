<div class="tab-pane active" ng-controller="AgentListController">
    <div class="row">
      <div class="col-md-3">
            @Html.Partial("_MemberMenu")
        </div>
        <div class="col-md-9">

            <div class="portlet box grey">
                <!-- Table Result Of Search-->
                <div class="portlet-title">
                    <div class="caption">
                        <i class="fa fa-bars"></i>Agent
                    </div>
                    <div class="actions">
                        <a href="" class="btn blue" ng-click="AddAgent()">
                            <i class="fa fa-pencil"></i>Add
                        </a>
                        <div class="btn-group">
                            <a class="btn default" href="" data-toggle="dropdown">Columns <i class="fa fa-angle-down"></i>
                            </a>
                            <div id="tblUser_column_toggler" class="dropdown-menu hold-on-click dropdown-checkboxes pull-right">
                                <label>
                                    <input type="checkbox" checked data-column="0">First Name</label>
                                <label>
                                    <input type="checkbox" checked data-column="1">Last Name</label>
                                <label>
                                    <input type="checkbox" checked data-column="2">Status</label>
                                <label>
                                    <input type="checkbox" checked data-column="3">Activated Date</label>
                                <label>
                                    <input type="checkbox" checked data-column="4">Chauffer Lic</label>
                                <label>
                                    <input type="checkbox" checked data-column="5">Mobile Phone</label>
                                  <label>
                                    <input type="checkbox" checked data-column="6">Home Phone</label>
                                 <label>
                                    <input type="checkbox" checked data-column="7">Vehicle Number</label>
                                
                            </div>
                        </div>
                    </div>
                </div>
                <div class="portlet-body">
                    <table class="table table-striped table-bordered table-hover" id="tblUser" aa-data="AgentList" paging-table>
                        <thead>
                            <tr>                                
                                <th>First Name
                                </th>
                                <th>Last Name
                                </th>
                                <th>Status
                                </th>
                                <th>Activated Date
                                </th>
                                <th>Chauffer Lic
                                </th>
                                <th>Mobile Phone
                                </th>
                                <th>Home Phone
                                </th>
                                <th>Vehicle Number
                                </th>
                                <th></th>
                            </tr>

                        </thead>
                        <tbody>                                  
                            <tr ng-repeat="agent in AgentList">                                      
                                <td>{{agent.Agent.FirstName}}
                                </td>
                                <td>{{agent.Agent.LastName}}
                                </td>
                                <td>{{agent.Agent.Status}}
                                </td>
                                <td>{{agent.Agent.ActiveDate}}
                                </td>
                                <td>{{agent.Agent.ChaufferLic}}
                                </td>
                                <td>{{agent.Agent.MobilePhone}}
                                </td>
                                <td>{{agent.Agent.HomePhone}}
                                </td>
                                <td>{{agent.VehicleName}}
                                </td>
                                <td>
                                    <a href="" ng-click="EditAgent(agent.Agent);">
                                        <i class="fa fa-edit"></i>
                                    </a>
                                    <a href="" ng-click="DelAgent(agent.Agent);"><i class="fa fa-ban"></i></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
