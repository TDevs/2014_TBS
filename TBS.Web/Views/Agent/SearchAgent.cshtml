<div  ng-controller="SearchAgentController">
    <!--Search Agent -->
    <div class="portlet box grey">
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-bars"></i>Search Agent
            </div>
        </div>
        <div class="portlet-body form">
            <form class="horizontal-form">
                <div class="form-body">
                    <div class="row">
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="col-md-4 control-label">First Name</label>
                                <input ng-model="AgentCritical.FirstName" name="FirstName" class="form-control" />
                            </div>
                        </div>
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="col-md-4 control-label">Last Name</label>
                                <input ng-model="AgentCritical.LastName" name="LastName" class="form-control" />
                            </div>
                        </div>
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="col-md-4 control-label">Medallion #</label>
                                <input ng-model="AgentCritical.Medallion" name="Medallion" class="form-control" />
                            </div>
                        </div>
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="col-md-4 control-label">Chauffer License #</label>
                                <input ng-model="AgentCritical.ChaufferLic" name="ChaufferLic" class="form-control" />
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="form-actions nobg fluid">
                <div class="col-md-offset-5 col-md-9">
                    <button class="btn btn green" type="button" ng-click="Seach();">FIND</button>
                    <button class="btn btn green" type="button" ng-click="Clear();">CLEAR</button>
                    <button class="btn default" type="button" ng-click="Cancel();">CANCEL</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Table Result Of Search-->

    <!--End Search Agent -->
    <div class="portlet box grey">
        <!-- Table Result Of Search-->
        <div class="portlet-title">
            <div class="caption">
                <i class="fa fa-bars"></i>Agent
            </div>
            <div class="actions">
                <a href="" class="btn blue" ng-click="Add()">
                    <i class="fa fa-pencil"></i>Add
                </a>
            </div>
        </div>
        <div class="portlet-body">
            <table class="table table-striped table-bordered table-hover" id="tblBank">
                <thead>
                    <tr>
                        <th style="width: 8px;">
                            <input type="checkbox" class="group-checkable" data-set="#tblBank.checkboxes" />
                        </th>
                        <th>Acc #
                        </th>
                        <th>First Name
                        </th>
                        <th>Last Name
                        </th>
                        <th>Medallion #
                        </th>
                        <th>Chauffer Lic
                        </th>

                        <th></th>
                    </tr>

                </thead>
                <tbody>
                    <tr ng-show="SearchAgentList.length<=0">
                        <td colspan="10" style="text-align: center">No Records </td>
                    </tr>
                    <tr ng-repeat="agent in SearchAgentList">
                        <th style="width: 8px;">
                            <input type="checkbox" class="group-checkable" data-set="#tblBank.checkboxes" />
                        </th>
                        <td>{{agent.AccountNumber}}
                        </td>
                        <td>{{agent.FirstName}}
                        </td>
                        <td>{{agent.LastName}}
                        </td>
                        <td>{{agent.Medallion}}
                        </td>
                        <td>{{agent.ChaufferLic}}
                        </td>
                        <td>
                            <a href="" ng-click="Edit(agent);">
                                <i class="fa fa-edit"></i>
                            </a>
                            <a href="" ng-click="Delete(agent);">
                                <i class="fa fa-ban"></i>
                            </a>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
