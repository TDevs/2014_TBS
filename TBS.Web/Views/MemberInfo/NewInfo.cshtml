<link href="/assets/css/style-metronic.css" rel="stylesheet" type="text/css" />
<link href="/assets/css/style.css" rel="stylesheet" type="text/css" />
<link href="/assets/css/style-responsive.css" rel="stylesheet" type="text/css" />
<link href="/assets/css/plugins.css" rel="stylesheet" type="text/css" />
<link href="/assets/css/themes/default.css" rel="stylesheet" type="text/css" id="style_color" />
<link href="/assets/css/custom.css" rel="stylesheet" type="text/css" />

<form class="row" ng-controller="CompanyController" name="frm" novalidate>
    <div class="col-md-6">
        <div class="portlet box">
            <div class="portlet-body form">
                <div class="form-horizontal">
                    <div class="form-body">
                        <div class="form-group">
                            <label class="col-md-5 control-label">Account</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Name</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Name is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Address 1</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Address is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Address 2</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">City</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">City is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">State/Zip</label>
                            <div class="col-md-3">
                                <select class="form-control" ng-model="Company.State" name="State" required>
                                    <option ng-repeat="state in States" ng-value="state">{{state}}</option>
                                </select>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Zip.$error.number&& frm.$dirty">State is required.
                                </span>
                            </div>
                            <div class="col-md-3">
                                <input type="number" class="form-control" ng-model="Company.Zip" name="Zip" input-mask="{mask:'99999'}" ng-type="phone" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Zip.$error.number&& frm.$dirty">Zip is required.
                                </span>
                            </div>
                        </div>

                        <div class="form-group ">
                            <label class="col-md-5 control-label"></label>
                            <div class="col-md-6">
                                <div class="checkbox">
                                    <label>
                                        <input type="checkbox" ng-model="Company.UseCompanyAddress" name="UseCompanyAddress">
                                        Use This Address For Billing?
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">IRIS #</label>
                            <div class="col-md-6">
                                <input type="number" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Primary Phone</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Primary Phone is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Phone 2</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Fax</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Email</label>
                            <div class="col-md-6">
                                <input type="email" class="form-control" name="Name" ng-model="Company.Name">
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Email invalid
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Article</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Member Account</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Fein #</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Incorporation Date</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Status</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Date Joined</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!--Member Preferences-->
    <div class="col-md-6">
        <div class="portlet box">
            <div class="portlet-body form">
                <div class="form-horizontal">
                    <div class="form-body">

                        <div class="form-group">
                            <label class="col-md-5 control-label"><b>Member Preferences</b></label>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Prefered Payment Type</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Late Breaks</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name" required>
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Name.$error.required">Account is required
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Default Worker Comp (Weekly)</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Registered Agent Name</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Address 1</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Address 2</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">City</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">State/Zip</label>
                            <div class="col-md-3">
                                <select class="form-control" ng-model="Company.State" name="State">
                                    <option ng-repeat="state in States" ng-value="state">{{state}}</option>
                                </select>
                            </div>
                            <div class="col-md-3">
                                <input type="number" class="form-control" placeholder="Enter text" ng-model="Company.Zip" name="Zip" input-mask="{mask:'99999'}" ng-type="phone">
                                <span style="color: #ff0000" ng-show="IsSubmitted && frm.Zip.$error.number&& frm.$dirty">Zip must be a number.
                                </span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label">Receipt message</label>
                            <div class="col-md-6">
                                <textarea class="form-control" name="Name" ng-model="Company.Name">      </textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-5 control-label"><b>Stockholder</b></label>
                        </div>
                        <!--Stockholder Grid-->
                        <div class="form-group">
                            <!--Grid-->
                            <label class="col-md-2 control-label"></label>
                            <div class="col-md-9">
                                <table class="table table-striped table-bordered table-advance table-hover">
                                    <thead>
                                        <tr>
                                            <th>
                                                <i class="fa fa-briefcase"></i>Name             
                                            </th>
                                            <th>
                                                <i class="fa fa-bookmark"></i>%
                                            </th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>
                                                Pixel Ltd   ANHV  fsdkfjsalk fjlska                                                    
                                            </td>
                                            <td>10       										 
                                            </td>
                                            <td>
                                                <a href="#" class="btn default btn-xs green-stripe">Edit
                                                </a>
                                                <a href="#" class="btn default btn-xs green-stripe">Delete
                                                </a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                                <!--Enter value-->
                                <div class="form-group">
                                    <div class="col-md-5">
                                        <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                                    </div>
                                    <div class="col-md-1">
                                        <input type="text" class="form-control" name="Name" ng-model="Company.Name">
                                    </div>
                                    <label class="col-md-1 control-label">%</label>
                                    <div class="col-md-3">
                                        <input type="button" value="Add Stockholder" class="btn btn-default btn green" />
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>
</form>

